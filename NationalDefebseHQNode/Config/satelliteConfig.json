{
    "communication": {
        "defaultPort": 8080,
            "protocol": "TCP",
                "timeoutMs": 10000,
                    "retries": 3,
                        "handshakeKey": "secure-handshake-key"
    },
    "satellites": [
        {
            "id": "SAT-001",
            "name": "DefenseComm-Alpha",
            "endpoint": "satellite-alpha.defense-network.com"
        },
        {
            "id": "SAT-002",
            "name": "DefenseComm-Beta",
            "endpoint": "satellite-beta.defense-network.com"
        },
        {
            "id": "SAT-003",
            "name": "DefenseComm-Gamma",
            "endpoint": "satellite-gamma.defense-network.com"
        }
    ],
        "security": {
        "encryptionAlgorithm": "AES-256",
            "hashAlgorithm": "SHA-256",
                "tlsVersion": "1.3"
    },
    "logging": {
        "logDirectory": "./Logs/Communication",
            "logFile": "satelliteCommLogs.json",
                "logLevel": "info"
    }
